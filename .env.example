# Environment variables for CRUB Course Team Management System
# Copy this file to .env and update with your actual credentials

# Google Sheets API Configuration
GOOGLE_SHEETS_BASE_URL=https://script.google.com/macros/s/YOUR_GOOGLE_SHEETS_DEPLOYMENT_ID/exec
GOOGLE_SHEETS_SECRET=your_secret_here

# Huayca API Configuration
HUAYCA_BASE_URL=https://huayca.crub.uncoma.edu.ar/catedras/1.0/rest/materias
HUAYCA_USERNAME=your_username_here
HUAYCA_PASSWORD=your_password_here

# FastAPI Configuration
FASTAPI_SECRET_KEY=your-secret-key-change-this-in-production
ADMIN_USERNAME=admin
ADMIN_PASSWORD=your_secure_password_here

# Development Settings
DEBUG=true
LOG_LEVEL=INFO
